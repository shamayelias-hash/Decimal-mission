    function restartGame() {
      currentLevel = 0;
      totalStars = 0;
      selectedOption = null;
      usedQuestions = [];
      currentQuestionIndex = 0;
      showScreen('start-screen');
    }

    document.getElementById('start-btn').addEventListener('click', function() {
