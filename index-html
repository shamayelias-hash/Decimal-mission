    function showScreen(screenId) {
      document.querySelectorAll('.screen').forEach(screen => {
        screen.classList.remove('active');
      });
      document.getElementById(screenId).classList.add('active');
    }

    function updateProgress() {
      const progress = ((currentLevel + 1) / levels.length) * 100;
      document.getElementById('progress-fill').style.width = progress + '%';
      document.getElementById('progress-fill').textContent = Math.round(progress) + '%';
      document.getElementById('progress-text').textContent = 'Ù…Ø±Ø­Ù„Ù‡ ' + (currentLevel + 1) + ' Ø§Ø² ' + levels.length;
    }

    function updateStars() {
      const starsHTML = 'â­'.repeat(totalStars);
      document.getElementById('stars-collected').textContent = starsHTML || 'Ù‡Ù†ÙˆØ² Ø³ØªØ§Ø±Ù‡â€ŒØ§ÛŒ Ù†Ø¯Ø§Ø±ÛŒ!';
    }

    function loadLevel() {
      const level = levels[currentLevel];
      
      if (!usedQuestions[currentLevel]) {
        usedQuestions[currentLevel] = [];
      }
      
      if (usedQuestions[currentLevel].length >= level.questions.length) {
        usedQuestions[currentLevel] = [];
      }
      
      let availableQuestions = [];
      level.questions.forEach(function(q, index) {
        if (!usedQuestions[currentLevel].includes(index)) {
          availableQuestions.push(index);
        }
      });
      
      currentQuestionIndex = availableQuestions[Math.floor(Math.random() * availableQuestions.length)];
      usedQuestions[currentLevel].push(currentQuestionIndex);
      
      const question = level.questions[currentQuestionIndex];

      document.getElementById('level-number').textContent = level.icon + ' Ù…Ø±Ø­Ù„Ù‡ ' + level.number;
      document.getElementById('level-title').textContent = level.title;
      document.getElementById('mission-text').textContent = level.mission;
      document.getElementById('question-text').textContent = question.text;

      const optionsContainer = document.getElementById('options-container');
      optionsContainer.innerHTML = '';

      question.options.forEach(function(option, index) {
        const btn = document.createElement('button');
        btn.className = 'btn-option';
        btn.textContent = option;
        btn.onclick = function() { selectOption(index); };
        optionsContainer.appendChild(btn);
      });

      document.getElementById('feedback-box').classList.remove('show');
      document.getElementById('check-btn').style.display = 'inline-block';
      document.getElementById('next-btn').style.display = 'none';
      selectedOption = null;

      updateProgress();
      updateStars();
    }

    function selectOption(index) {
      selectedOption = index;
      const options = document.querySelectorAll('.btn-option');
      options.forEach(function(btn, i) {
        btn.classList.remove('selected');
        if (i === index) {
          btn.classList.add('selected');
        }
      });
    }

    function checkAnswer() {
      if (selectedOption === null) {
        const feedbackBox = document.getElementById('feedback-box');
        feedbackBox.className = 'feedback-box incorrect show';
        feedbackBox.innerHTML = '<div class="feedback-icon">âš ï¸</div><div>' + messages.selectOption + '</div>';
        return;
      }

      const level = levels[currentLevel];
      const question = level.questions[currentQuestionIndex];
      const feedbackBox = document.getElementById('feedback-box');
      const options = document.querySelectorAll('.btn-option');

      if (selectedOption === question.correct) {
        totalStars++;
        feedbackBox.className = 'feedback-box correct show';
        feedbackBox.innerHTML = '<div class="feedback-icon">âœ…</div><div>' + messages.correct + '</div>';
        options[selectedOption].classList.add('correct');
        
        document.getElementById('check-btn').style.display = 'none';
        document.getElementById('next-btn').style.display = 'inline-block';
        updateStars();
      } else {
        feedbackBox.className = 'feedback-box incorrect show';
        feedbackBox.innerHTML = '<div class="feedback-icon">âŒ</div><div>' + messages.incorrect + '</div>';
        options[selectedOption].classList.add('incorrect');
        
        setTimeout(function() {
          options[selectedOption].classList.remove('incorrect', 'selected');
          feedbackBox.classList.remove('show');
          selectedOption = null;
        }, 2000);
      }
    }

    function nextLevel() {
      if (currentLevel < levels.length - 1) {
        currentLevel++;
        loadLevel();
      } else {
        showEndScreen();
      }
    }

