    let currentLevel = 0;
    let selectedOption = null;
    let totalStars = 0;
    let usedQuestions = [];
    let currentQuestionIndex = 0;
    const totalQuestions = 4;

    const messages = {
      correct: "Ø¢ÙØ±ÛŒÙ†! Ù¾Ø§Ø³Ø®Øª Ø¯Ø±Ø³ØªÙ‡! ğŸ‰",
